{
    "name": "laravel-russian/laravel-sluggable",
    "description": "Generate slugs when saving Eloquent models",
    "keywords": [
        "laravel-russian",
        "laravel-sluggable"
    ],
    "homepage": "https://github.com/laravel-russian/laravel-sluggable",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@laravel-russian.be",
            "homepage": "https://laravel-russian.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/database": "^8.0|^9.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.23|^7.0",
        "laravel-russian/laravel-translatable": "dev-master",
        "pestphp/pest": "^1.20"
    },
    "autoload": {
        "psr-4": {
            "LaravelRussian\\Sluggable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelRussian\\Sluggable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../laravel-translatable"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
